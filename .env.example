# Example .env file for Crypto Analyzer
# Copy this file to .env and add your API keys

# Binance API Keys (optional for public endpoints)
BINANCE_API_KEY=
BINANCE_API_SECRET=

# CoinGecko API (Optional - for enhanced market data)
COINGECKO_API_KEY=

# CoinMarketCap API (Optional)
COINMARKETCAP_API_KEY=

# CryptoPanic API (Optional - for news aggregation)
CRYPTOPANIC_API_KEY=

# Twitter API v2 (Optional - for social sentiment)
TWITTER_BEARER_TOKEN=

# Reddit API (Optional - for social sentiment)
REDDIT_CLIENT_ID=
REDDIT_CLIENT_SECRET=
REDDIT_USER_AGENT=CryptoAnalyzer/1.0

# Etherscan API (Optional - for Ethereum on-chain data)
ETHERSCAN_API_KEY=

# BSCScan API (Optional - for BSC on-chain data)
BSCSCAN_API_KEY=

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Note: Most features work without API keys
# Binance public API requires no authentication

